{
  "language_models": {
    "google": {
      "available_models": [
        {
          "name": "gemini-1.5-pro",
          "display_name": "Gemini 1.5",
          "max_tokens": 1000000
        }
      ]
    }
  },
  "assistant": {
    "default_model": {
      "provider": "google",
      "model": "gemini-1.5-pro"
    },
    "version": "2"
  },
  "base_keymap": "VSCode",
  "vim_mode": true,
  "buffer_font_family": "CaskaydiaCove Nerd Font Mono",
  "buffer_font_size": 16,
  "ui_font_size": 16,
  "relative_line_numbers": true,
  "buffer_font_features": {
    "calt": false
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "theme": {
    "mode": "system",
    "light": "One Dark",
    "dark": "One Dark"
  },
  "terminal": {
    "env": {
      "EDITOR": "zed --wait"
    }
  }
}
